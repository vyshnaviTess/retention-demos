{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "noEmit": true,
    "types": [
      "jest",
      "node"
    ]
  },
  "exclude": ["node_modules", "build"],
  "extends": "expo/tsconfig.base"
}

